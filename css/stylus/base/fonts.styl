@font-face
  font-family 'NeoSans'
  src url("/fonts/NeoSansStd-Bold.eot") format('eot'),
      url("/fonts/NeoSansStd-Bold.woff") format('woff'),
      url("/fonts/NeoSansStd-Bold.ttf") format('truetype'),
      url("/fonts/NeoSansStd-Light.svg#NeoSansStd-Bold") format('svg')
  font-weight 700
  font-style normal

